{
    "name":         "qaReview",
    "description":  "Simple extensible helper for code reviews",
    "repositories": [ {
        "type": "vcs",
        "url":  "https://github.com/Qafoo/REST-Micro-Framework"
    }, {
        "type":    "package",
        "package": {
            "name":    "twitter/bootstrap",
            "version": "0.1",
            "source": {
                "url": "https://github.com/twitter/bootstrap/",
                "type": "git",
                "reference": "origin/master"
            }
        }
    }, {
        "type":    "package",
        "package": {
            "name":    "arbitracker/systemProcess",
            "version": "0.1",
            "source": {
                "url": "https://github.com/Arbitracker/system-process.git",
                "type": "git",
                "reference": "origin/master"
            }
        }
    }, {
        "type":    "package",
        "package": {
            "name":    "pdepend/staticReflection",
            "version": "0.1",
            "source": {
                "url": "https://github.com/manuelpichler/staticReflection.git",
                "type": "git",
                "reference": "origin/master"
            }
        }
    } ],
    "require":      {
        "Qafoo/RMF": "dev-master",
        "twitter/bootstrap": "0.1",
        "arbitracker/systemProcess": "0.1",
        "pdepend/staticReflection": "0.1",
        "zetacomponents/base": "*",
        "zetacomponents/graph": "*",
        "twig/twig": "1.6.0"
    },
    "autoload": {
        "psr-0": {
            "SystemProcess": ["src/library/arbitracker/systemProcess/src/main/php/"],
            "pdepend\\reflection": ["src/library/pdepend/staticReflection/src/main/php/"]
        }
    },
    "config":       {
        "vendor-dir": "src/library"
    }
}
